{
	"manifest_version": 2,

	"name": "Yelp ext",
	"description": "This allows you to see yelp reviews of a place mentioned on a webpage",
	"version":"1.0",


	"page_action": {
		"default_icon": "icon16.png",
		"default_popup": "popup.html",
		"default_title": "Find Yelp reviews"
	},
	
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

	"permissions": [
		"activetabs"
	]
}